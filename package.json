{
  "name": "character-hash",
  "version": "0.0.1",
  "description": "Deterministically generate CJK-styled SVG glyphs from a 16-hex-character string",
  "license": "The Unlicense",
  "main": "./index.js",
  "bin": {
    "character-hash": "./index.js"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node index.js",
    "test": "node index.js 0000000000001a2b > /tmp/character-hash-0000000000001a2b.svg && echo \"wrote /tmp/character-hash-0000000000001a2b.svg\""
  },
  "dependencies": {
    "yargs": "^18.0.0"
  }
}
